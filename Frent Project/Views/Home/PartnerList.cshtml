<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="stylesheet" href="~/CSS/PartnerList.css" type="text/css">
    <title>FRENT | Partner</title>
    <script
  src="https://code.jquery.com/jquery-3.6.0.js"
  integrity="sha256-H+K7U5CnXl1h5ywQfKtSj8PCmoN9aaq30gDh27Xc0jk="
  crossorigin="anonymous"></script>
  </head>
<body>

    <nav class="nav pink">
        <div class="container">
            <div class="logo">
                <a asp-controller="Home" asp-action="Home">
                    <img src="~/Media/Logo_Home.png" alt="">
                </a>
            </div>
            <span class="search-bar">
                <input type="text" placeholder="Search For Partner" name="" id="">
                <a asp-controller="Home" asp-action="PartnerList"><img src="~/Media/Icon_search.png" alt=""></a>
            </span>
            <div id="mainListDiv" class="main_list">
                <ul class="navlinks">
                    <li>
                        <div class="nav-button">
                            <a asp-controller="Home" asp-action="Order"><img src="~/Media/Buy.png" alt=""></a>
                        </div>
                    </li>
                    <li>
                        <div class="nav-button">
                            <a asp-controller="Home" asp-action="ChatList"><img src="~/Media/Chat1.png" alt=""></a>
                        </div>
                    </li>
                    <li class="nav-button" id="nav-profile">
                        <div>
                            <a asp-controller="Home" asp-action="Profile"><img src="~/Media/Profile.png" alt=""></a>
                        </div>
                    </li>
                    <li class="nav-button">
                        <div class="nav-dropdown">
                            <img  src="~/Media/Drop.png" alt="">
                        </div>
                        <div class="dropped">
                            <a asp-controller="Home" asp-action="Settings2">Setting</a>
                            <a class="dropped-content2" asp-controller="Home" asp-action="ContactUS">Contact Us</a>
                            <a asp-controller="Home" asp-action="Index">Log Out</a>
                        </div>
                    </li>
                </ul>
            </div>
        </div>
    </nav>

    <div class="status-content" id="completed-content">
        <div class="partner-completed">
            <a style="color: black; text-decoration: none;" asp-controller="Home" asp-action="partnerDetails"><div class="section1-completed">
                <div class="profile-rate">
                    <div><img src="~/media/FemaleGender.png" alt="" id="profile1"></div>
                    <div><img src="~/Media/Stars.png" alt="" class="stars" id="stars1"></div>
                </div>
            
                <div class="profile-completed">
                    <h1>Jesse Field</h1>
                    <h2>Travelling Friend, Offline</h2>
                    <span>
                        <img src="~/Media/Location.svg" alt="">
                        <h3>Jakarta Pusat</h3>
                    </span>
                    <h3>Rp 450.000</h3>
                </div>
        </div>
            </a>
            <div class="section2-completed">
                <a asp-controller="Home" asp-action="Payment" class="button-completed">
                    Rent
                </a>
            </div>
        </div>
        <div class="partner-completed sec2">
            <div class="section1-completed">
                <div class="profile-rate">
                    <div><img src="~/media/MaleGender.png" alt="" id="profile1"></div>
                    <div><img src="~/Media/Stars.png" alt="" class="stars" id="stars1"></div>
                </div>
                <div class="profile-completed">
                    <h1>Jesse Field</h1>
                    <h2>Travelling Friend, Offline</h2>
                    <span>
                        <img src="~/Media/Location.svg" alt="">
                        <h3>Jakarta Pusat</h3>
                    </span>
                    <h3>Rp 450.000</h3>
                </div>
            </div>
            <div class="section2-completed">
                <a asp-controller="Home" asp-action="Payment" class="button-completed">
                    Rent
                </a>
            </div>
        </div>
        <div class="partner-completed sec3">
            <div class="section1-completed">
                <div class="profile-rate">
                    <div><img src="~/media/FemaleGender.png" alt="" id="profile1"></div>
                    <div><img src="~/Media/Stars.png" alt="" class="stars" id="stars1"></div>
                </div>
                <div class="profile-completed">
                    <h1>Jesse Field</h1>
                    <h2>Travelling Friend, Offline</h2>
                    <span>
                        <img src="~/Media/Location.svg" alt="">
                        <h3>Jakarta Pusat</h3>
                    </span>
                    <h3>Rp 450.000</h3>
                </div>
            </div>
            <div class="section2-completed">
                <a asp-controller="Home" asp-action="Payment" class="button-completed">
                    Rent
                </a>
            </div>
        </div>
    </div>
</body>
<footer>
    <div class="social-media-footer">
        <a href="https://www.facebook.com"><img src="~/Media/FB.png" alt=""></a>
        <a href="https://www.twitter.com"><img src="~/Media/TWITTER.png" alt=""></a>
        <a href="https://www.instagram.com"><img src="~/Media/IG.png" alt=""></a>
    </div>
